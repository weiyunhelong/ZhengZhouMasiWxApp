/* pages/jiyin/list.wxss */
page {
  background: #f2f2f2;
}

.block {
  width: 710rpx;
  height: 200rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 22rpx auto 0;
  display: flex;
}

.liev {
  width: 6rpx;
  height: 200rpx;
  background: linear-gradient(180deg, #124361, #0D6675);
  box-shadow: 0px 2rpx 7rpx 0rpx rgba(209, 64, 0, 0.35);
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}

.cover {
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx 30rpx 0 20rpx;
}

.infov {
  width: 475rpx;
  height: 66rpx;
  font-size: 26rpx;
  color: #124361;
}

.name {
  padding-top: 32rpx;
  font-size: 26rpx;
  color: #124361;
  line-height: 45rpx;
}

.optv {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.optcv {
  display: flex;
  font-size: 24rpx;
  color: #999999;
  line-height: 32rpx;
}

.icon1 {
  width: 23rpx;
  height: 21rpx;
  margin-right: 20rpx;
  margin-top: 8rpx;
}

.icon2 {
  width: 19rpx;
  height: 20rpx;
  margin-right: 20rpx;
  margin-top: 7rpx;
}

.icon3 {
  width: 21rpx;
  height: 20rpx;
  margin-right: 20rpx;
  margin-top: 7rpx;
}
.optnumh{
  color: #ff0000;
}